# environment: production,staging
# Set the environment variable NODE_ENV to 'production' for production environment
NODE_ENV= your_environment_here

# Database connection string PostgreSQL (Neon)
DATABASE_URL= your_database_connection_string_here

# Session secret
SESSION_SECRET= your_session_secret_here

# Razorpay API Keys (Live)
RAZORPAY_KEY_ID= your_razorpay_key_id_here
RAZORPAY_KEY_SECRET= your_razorpay_key_secret_here  

# Paytm merchant details
# For production, replace with your actual Paytm merchant details
PAYTM_MERCHANT_ID= your_paytm_merchant_id_here
PAYTM_MERCHANT_KEY= your_paytm_merchant_key_here
PAYTM_WEBSITE= your_paytm_website_here
PAYTM_CHANNEL_ID= your_paytm_channel_id_here
PAYTM_INDUSTRY_TYPE= your_paytm_industry_type_here

# SendGrid API Key
SENDGRID_API_KEY= your_sendgrid_api_key_here

# Frontend URL
# This should be the URL of your frontend application
FRONTEND_URL= your_frontend_url_here

# OpenAI API Key
# This is used for AI features in the application
OPENAI_API_KEY= your_openai_api_key_here

REPL_ID= your_repl_id_here
SESSION_SECRET= your_session_secret_here